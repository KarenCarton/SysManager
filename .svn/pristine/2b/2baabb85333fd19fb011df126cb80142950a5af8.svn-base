.vie_table {
	width: 98%;
	background: #f1f1f1;
	border-collapse: collapse;
	border: #adc3d7 1px solid;
	border-spacing: 0px;
	padd: expression(this. cellpadding = 0);
	line-height: 20px;
}

.vie_table tr {
	height: 25px;
	font-family: arial;
}

.vie_table td {
	border: #adc3d7 1px solid;
	line-height: 24px;
	background-color: white;
	font-size: 5px;
	text-align: center;
}

.link-right {
	width: 1px;
	height: 99%;
	border-right: dashed #ACC0D8 1px;
	float: right;
}

.btnDiv {
	height: 30px;
	width: 32%;
	position: fixed;
	border-top: 1px #CCC dashed;
	text-align: center;
	vertical-align: middle;
}