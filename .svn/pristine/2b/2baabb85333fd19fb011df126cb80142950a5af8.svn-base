#contain  div { /* �����Ӽ��� */
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}

input {
	outline-style: none;
	border: 1px solid #CCC;
	border-radius: 3px;
	padding: 5px;
	font-family: "΢���ź�";
}

input:focus {
	border-color: #66afe9;
	outline: 0;
	-webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px
		rgba(102, 175, 233, .6);
	box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075), 0 0 8px
		rgba(102, 175, 233, .6);
}

input::-webkit-input-placeholder{
	color: #ccc;
}

.a_Btn {
	box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.2);
	text-shadow: 0 1px 0 rgba(0, 0, 0, 0.2);
	padding: 5px 10px;
	border-radius: 5px;
	color: #FFFFFF;
	background-color: #61ba9e; /*#61ba9e*/
	border-color: #4fb393; /*#4fb393*/
	display: inline-block;
	cursor: pointer;
	border: 1px solid transparent;
	white-space: nowrap;
}

.a_Btn:hover {
	background-color: #46A285; /*#46A285*/
	border-color: #3B8970; /*#3B8970*/
}

