/* 小tab标签样式 */
.littleTab {
	background: url(tabbj.gif) repeat-x;
	list-style-type: none;
	margin: 0px;
	padding: 0px;
	width: 100%;
	height: 28px;
}

.littleTab_li {
	float: left;
	width: 60px;
	height: 28px;
	line-height: 28px;
	cursor: pointer;
	text-align: center;
}

.littleTab_li a {
	float: left;
	width: 100%;
	height: 28px;
	line-height: 28px;
	cursor: pointer;
	text-align: center;
	text-decoration: none;
	color: black;
}

.littleTab_liActive {
	background: url(tab_l.gif) top left no-repeat;
	height: 28px;
	color: #fff;
	margin-left: 5px;
}

.littleTab_liActive a {
	background: url(tab_r.gif) top right no-repeat;
	height: 28px;
	color: #fff;
}

