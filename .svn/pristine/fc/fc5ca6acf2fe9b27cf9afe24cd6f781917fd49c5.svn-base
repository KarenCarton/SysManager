}
::-webkit-scrollbar-thumb{/*滚动条默认显示样式*/
	background-color:#eeeeee;
	height:40px;
	outline-offset:-2px;
	outline:2px soild #fff;
	-webkit-border-radius:4px;
	border:2px solid #fff;
	border-radius:10px;
	-webkit-box-shandow:inset 0 0 6px rgba(0,0,0,3);
}
::-webkit-scrollbar-thumb:hover{/*鼠标点击滚动条样式*/
	background-color:#c0c0c0;
	height:50px;
	-webkit-border-radius:10px;
	
}
::-webkit-scrollbar-track{
	-webkit-box-shandow:inset 0 0 6px rgba(0,0,0,3);
	border-radius:10px;
	background-color:#F5F5F5;
}
::-webkit-scrollbar{/*滚动条大小*/
	width:12px;
	height:8px;
}
::-webkit-scrollbar-track-piece{/*滚动条背景样式*/
	backgroud-color:#fff;
	-webkit-border-radius:0;
}