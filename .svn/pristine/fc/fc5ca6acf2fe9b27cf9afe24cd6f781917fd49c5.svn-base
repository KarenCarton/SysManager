	color: #2e6ab1;
	line-height: 20px;
	margin-left: 3px;
}

.blockPageSep .blockPageSep_normal:hover {
	background: #9aafe5;
	border: #2b66a5 1px solid;
	color: #000;
}

.blockPageSep .blockPageSep_selected {
	background: #2e6ab1;
	border: #008 1px solid;
	color: #fff;
	line-height: 20px;
	margin-left: 3px;
}

